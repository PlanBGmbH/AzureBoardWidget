{
    "manifestVersion": 1,
    "id": "vsts-extensions-myExtensions",
    "version": "1.0.47",
    "name": "My First Set of Widgets",
    "description": "Samples containing different widgets extending dashboards",
    "publisher": "LucaEbert",
    "categories":  [
        "Azure Boards"
    ],
    "identity":  {
        "internalName":  "MyFirstVsixExtension"
    },
    "overview":  "overview.md",
    "priceCategory":  "free",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "/node_modules/vss-web-extension-sdk/img/logo.png"
    },
    "contributions": [
        {
            "id":  "HelloWorldWidget",
            "type":  "ms.vss-dashboards-web.widget",
            "targets":  "ms.vss-dashboards-web.widget-catalog",
            "properties":  "@{name=Hello World Widget; description=My first widget; catalogIconUrl=img/CatalogIcon.png; previewImageUrl=img/preview.png; uri=hello-world.html; supportedSizes=System.Object[]}"
        }
    ],
    "files": [
        {
            "path": "hello-world.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/img",
            "addressable": true
        }
    ]
}